<http://www.invincea.com/ontologies/icas/1.0/metadata>
    dc:creator "Invincea Labs"@en ;
    dc:date "2015" ;
    dc:description ""@en ;
    a owl:Ontology ;
    rdfs:comment ""@en ;
    rdfs:label "Metadata Ontology"@en ;
